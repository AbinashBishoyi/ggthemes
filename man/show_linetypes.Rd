\name{show_linetypes}
\alias{show_linetypes}
\title{Show linetypes}
\usage{
  show_linetypes(linetypes)
}
\arguments{
  \item{linetypes}{A character vector of linetypes. See
  \code{\link{par}}.}
}
\description{
  A quick and dirty way to show linetypes for a palette.
}
\examples{
show_shapes(linetype_pal()(3))
}
\seealso{
  \code{\link{show_col}}, \code{\link{show_linetypes}}
}

